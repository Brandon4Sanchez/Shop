@model Project_MVC.Logica.Models.BindingModels.ParcialCreateBindingModels

    <form method="post" action="/Parcial/Create">
        <div class="form-row">
            <div class="col-md-4">
                @Html.LabelFor(m => m.Name)
                @Html.EditorFor(m => m.Name, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(m => m.Name, "", new { @class = "text-danger" })
            </div>
            <div class="col-md-4">
                @Html.LabelFor(m => m.LastName)
                @Html.EditorFor(m => m.LastName, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(m => m.LastName, "", new { @class = "text-danger" })
            </div>
            <div class="col-md-4">
                @Html.LabelFor(m => m.Age)
                @Html.EditorFor(m => m.Age, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(m => m.Age, "", new { @class = "text-danger" })
            </div>
        </div>

        <div class="form-row">
            <div class="col-md-4">
                @Html.LabelFor(m => m.Id)
                @Html.EditorFor(m => m.Id, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(m => m.Id, "", new { @class = "text-danger" })
            </div>
            <div class="col-md-4">
                @Html.LabelFor(m => m.PlaceOfBirth)
                @Html.EditorFor(m => m.PlaceOfBirth, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(m => m.PlaceOfBirth, "", new { @class = "text-danger" })
            </div>
        </div>

        <div class="form-row">
            <div class="col-md-4">
                @Html.LabelFor(m => m.Address)
                @Html.EditorFor(m => m.Address, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(m => m.Address, "", new { @class = "text-danger" })
            </div>
            <div class="col-md-4">
                @Html.LabelFor(m => m.Phone)
                @Html.EditorFor(m => m.Phone, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(m => m.Phone, "", new { @class = "text-danger" })
            </div>
        </div>

        <div class="form-row">
            <div class="col-md-4">
                @Html.LabelFor(m => m.Salary)
                @Html.EditorFor(m => m.Salary, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(m => m.Salary, "", new { @class = "text-danger" })
            </div>
            <div class="col-md-4">
                @Html.LabelFor(m => m.WorkedDays)
                @Html.EditorFor(m => m.WorkedDays, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(m => m.WorkedDays, "", new { @class = "text-danger" })
            </div>
        </div>
        <div class="form-row">
            <div class="col-md-4">
                @Html.LabelFor(m => m.SpouseSalary)
                @Html.EditorFor(m => m.SpouseSalary, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(m => m.SpouseSalary, "", new { @class = "text-danger" })
            </div>
            <div class="col-md-4">
                @Html.LabelFor(m => m.DaysWorkedTogether)
                @Html.EditorFor(m => m.DaysWorkedTogether, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(m => m.DaysWorkedTogether, "", new { @class = "text-danger" })
            </div>
        </div>
        <h2></h2>

        <input type="submit" value="Create" class="btn btn-primary" />


        @if (ViewBag.Data != null)
        {
            <div class="form-row">
                <div class="col.md-12">

                    <table class="table">
                        <tr>
                            <td>Id</td>
                            <td>Name</td>
                            <td>LastName</td>
                            <td>Address</td>
                            <td>Age</td>
                            <td>PlaceOfBirth</td>
                            <td>Phone</td>
                            <td>Subsidy</td>
                        </tr>
                        @foreach (Project_MVC.Logica.Models.ViewModels.ParcialCreateViewModel item in ViewBag.Data)
                        {
                            <tr>
                                <td>@item.Id</td>
                                <td>@item.Name</td>
                                <td>@item.LastName</td>
                                <td>@item.Address</td>
                                <td>@item.Age</td>
                                <td>@item.PlaceOfBirth</td>
                                <td>@item.Phone</td>
                                <td>@item.Subsidy</td>
                            </tr>
                        }
                    </table>
                </div>
            </div>
        }
    </form>

